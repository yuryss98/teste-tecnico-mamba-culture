  "main": "./dist/main/server.js",
  "scripts": {
    "clean-dist": "rimraf ./dist || true",
    "build": "npm run clean-dist && tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "nodemon ./src/main/server.ts",
    "lint": "eslint . --ext .js,.ts",
    "test": "jest",
    "test:unit": "npm test -- -c jest-unit.config.js",
    "test:e2e": "npm test -- -c jest-e2e.config.js",
    "test:coverage": "jest --coverage",
    "prisma:migrate": "prisma migrate dev && prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.6.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.30.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }

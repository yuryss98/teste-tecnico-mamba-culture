{
  "openapi": "3.0.0",
  "info": {
    "title": "Campaigns Api",
    "version": "1.0.0"
  },
  "paths": {
    "/api/category/findAll": {
      "get": {
        "tags": [
          "category"
        ],
        "summary": "Find all categories",
        "responses": {
          "200": {
            "description": "All categories",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "body": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "example": 1
                          },
                          "name": {
                            "type": "string",
                            "example": "Campanhas de Vendas"
                          },
                          "createdAt": {
                            "type": "string",
                            "example": "2024-09-26T22:24:08.319Z"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
